    "querystring": {
        "select": "SELECT L.LEAD_NO AS LeadNo, L.ORI_OFFICE_CODE AS OriOfficeCode, LC.CUST_NAME AS CustName,  LA.FULL_ASSET_NAME AS FullAssetName, L.ORDER_NO AS OrderNo, L.LEAD_ID AS LeadId, L.MR_LEAD_SOURCE_CODE AS  MrLeadSourceCode , RM.DESCR AS LeadSource, L.LEAD_STAT, RSTAT.DESCR AS LeadStatus, RS.DESCR AS Descr, L.LEAD_STEP AS LeadStep, WF.WF_TASK_LIST_ID AS WfTaskListId, RO.OFFICE_NAME AS OfficeName",
        "from": "FROM  dbo.LEAD L WITH(NOLOCK) LEFT JOIN dbo.LEAD_CUST LC WITH(NOLOCK) ON LC.LEAD_ID = L.LEAD_ID LEFT JOIN dbo.LEAD DL WITH(NOLOCK) ON DL.LEAD_ID = L.DUPLICATE_LEAD_ID LEFT JOIN dbo.LEAD_ASSET LA WITH(NOLOCK) ON LA.LEAD_ID = LC.LEAD_ID LEFT JOIN [V_REF_MASTER] RM ON RM.MASTER_CODE = L.MR_LEAD_SOURCE_CODE AND RM.REF_MASTER_TYPE_CODE = 'LEAD_SOURCE' LEFT JOIN [dbo].[V_REF_STATUS] RS ON RS.STATUS_GRP_CODE = 'LEAD_STEP' AND RS.REF_STATUS_CODE = L.LEAD_STEP LEFT JOIN [dbo].[V_REF_STATUS] RSTAT ON RSTAT.STATUS_GRP_CODE = 'LEAD_STAT' AND RSTAT.REF_STATUS_CODE = L.LEAD_STAT LEFT JOIN (SELECT VWF.TRANSACTION_CODE,MAX(VWF.WF_TASK_LIST_ID) AS WF_TASK_LIST_ID FROM dbo.V_WF_TASK_LIST VWF WITH(NOLOCK) WHERE VWF.WF_CODE = 'WF_LEAD' GROUP BY VWF.TRANSACTION_CODE) WF ON L.LEAD_NO = WF.TRANSACTION_CODE JOIN dbo.V_REF_OFFICE RO WITH(NOLOCK) ON L.ORI_OFFICE_CODE = RO.OFFICE_CODE"
    }
}