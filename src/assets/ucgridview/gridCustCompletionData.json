{
    "headerList": [
        {
            "type": "label",
            "position": "left",
            "label": "Subject",
            "name": "Subject"
        },
        {
            "type": "label",
            "position": "left",
            "label": "Name",
            "name": "CustName"
        },
        {
            "type": "label",
            "position": "left",
            "label": "Customer Relationship",
            "name": "MrCustRelationshipCode"
        },
        {
            "type": "label",
            "position": "center",
            "label": "Customer No",
            "name": "CustNo"
        },
        {
            "type": "label",
            "position": "center",
            "label": "Applicant No",
            "name": "ApplicantNo"
        },
        {
            "type": "label",
            "position": "center",
            "label": "Action",
            "name": "Action"
        }
    ],
    "bodyList": [
        {
            "type": "text",
            "position": "left",
            "property": "SubjectDescr"
        },
        {
            "type": "text",
            "position": "left",
            "property": "CustName"
        },
        {
            "type": "text",
            "position": "left",
            "property": "MrRelationshipCustDescr"
        },
        {
            "type": "text",
            "position": "left",
            "property": "CustNo"
        },
        {
            "type": "text",
            "position": "left",
            "property": "ApplicantNo"
        },
        {
            "type": "action",
            "position": "center",
            "action": [
                {
                    "type": "switch",
                    "case": [
                        {
                            "conditions": [
                                {
                                    "property": "MrCustTypeCode",
                                    "value": "PERSONAL",
                                    "restriction": "EQ"
                                }
                            ],
                            "result": {
                                "type": "url",
                                "path": "../Detail/Personal",
                                "param": [
                                    {
                                        "type": "AppCustId",
                                        "property": "AppCustId"
                                    }
                                ]
                            }
                        },
                        {
                            "conditions": [
                                {
                                    "property": "MrCustTypeCode",
                                    "value": "COMPANY",
                                    "restriction": "EQ"
                                }
                            ],
                            "result": {
                                "type": "url",
                                "path": "../Detail/Company",
                                "param": [
                                    {
                                        "type": "AppCustId",
                                        "property": "AppCustId"
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        }
    ]
}