    "querystring": {
      "select": "SELECT MCC.COLLATERAL_NO AS CollateralNo, MCC.FULL_ASSET_CODE AS FullAssetCode, MCC.FULL_ASSET_NAME AS FullAssetName, MCC.MANUFACTURING_YEAR AS ManufacturingYear, MCC.SERIAL_NO_1 AS SerialNo1, MCC.SERIAL_NO_2 AS SerialNo2, MCC.SERIAL_NO_3 AS SerialNo3",
      "from": "FROM dbo.MOU_CUST AS MC WITH(NOLOCK) INNER JOIN dbo.MOU_CUST_COLLATERAL AS MCC WITH(NOLOCK) ON MC.MOU_CUST_ID = MCC.MOU_CUST_ID",
      "where": "WHERE MC.MOU_STAT IN ( 'NEW', 'RJC', 'CAN', 'EXP')",
      "groupby": "GROUP BY MCC.COLLATERAL_NO, MCC.FULL_ASSET_CODE, MCC.FULL_ASSET_NAME, MCC.MANUFACTURING_YEAR, MCC.SERIAL_NO_1, MCC.SERIAL_NO_2, MCC.SERIAL_NO_3, MCC.SERIAL_NO_4, MCC.SERIAL_NO_5"
    },
