    ],
    "querystring": {
      "select": "SELECT AC.FULL_ASSET_NAME AS FullAssetName, AC.ASSET_TYPE_CODE AS AssetTypeCode, AC.COLLATERAL_VALUE_AMT AS CollateralValueAmt, ACR.OWNER_NAME AS CollateralOwner, AC.COLLATERAL_NOTES AS CollateralNotes, AC.SERIAL_NO_1 AS SerialNo1, AC.SERIAL_NO_2 AS SerialNo2, AC.SERIAL_NO_3 AS SerialNo3, AC.APP_COLLATERAL_ID AS AppCollateralId",
      "from": "FROM dbo.APP_COLLATERAL AC WITH(NOLOCK) INNER JOIN ( SELECT MAX(APP_COLLATERAL_ID) APP_COLLATERAL_ID FROM dbo.APP_COLLATERAL AA WITH(NOLOCK) GROUP BY AA.FULL_ASSET_NAME, AA.ASSET_TYPE_CODE, AA.COLLATERAL_VALUE_AMT, AA.SERIAL_NO_1, AA.SERIAL_NO_2 )QRY ON AC.APP_COLLATERAL_ID = QRY.APP_COLLATERAL_ID INNER JOIN dbo.APP_COLLATERAL_REGISTRATION ACR WITH(NOLOCK) ON QRY.APP_COLLATERAL_ID = ACR.APP_COLLATERAL_ID INNER JOIN dbo.APP A ON A.APP_ID = AC.APP_ID INNER JOIN dbo.APP_CUST ACU ON ACU.APP_ID = A.APP_ID"
    },
