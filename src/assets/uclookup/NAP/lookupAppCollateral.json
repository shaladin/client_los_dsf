    "querystring": {
      "select": "SELECT apctrl.FULL_ASSET_NAME AS FullAssetName, apctrl.ASSET_TYPE_CODE AS AssetTypeCode, apctrl.COLLATERAL_VALUE_AMT AS CollateralValueAmt, apctrlr.OWNER_NAME AS CollateralOwner, apctrl.COLLATERAL_NOTES AS CollateralNotes, apctrl.SERIAL_NO_1 AS SerialNo1, apctrl.SERIAL_NO_2 AS SerialNo2, apctrl.SERIAL_NO_3 AS SerialNo3, apctrl.APP_COLLATERAL_ID AS AppCollateralId",
      "from": "FROM dbo.APP_COLLATERAL apctrl WITH(NOLOCK) JOIN dbo.APP_COLLATERAL_REGISTRATION apctrlr WITH(NOLOCK) ON apctrl.APP_COLLATERAL_ID = apctrlr.APP_COLLATERAL_ID"
    },
