      "from": "FROM dbo.APP_COLLATERAL apctrl WITH(NOLOCK) JOIN dbo.APP_COLLATERAL_REGISTRATION apctrlr WITH(NOLOCK) ON apctrl.APP_COLLATERAL_ID = apctrlr.APP_COLLATERAL_ID"
    },
