    "querystring": {
      "select": "SELECT v.VENDOR_CODE AS ReferantorCode, v.VENDOR_NAME AS ReferantorName, v.MR_VENDOR_CATEGORY_CODE AS ReferantorType, v.IS_NPWP_EXIST AS IsNPWPExist, v.TAXPAYER_NO AS TaxPayerNo, v.TAXPAYER_NAME AS TaxPayerName, v.TAX_ID_NO AS TaxIdNo, va.ADDR AS Addr, va.CITY AS City, va.ZIPCODE AS ZipCode, va.AREA_CODE_1 AS Kecamatan, va.AREA_CODE_2 AS Kelurahan, va.AREA_CODE_3 AS RW, va.AREA_CODE_4 AS RT, v.MR_TAX_CALC_METHOD_CODE AS MrTaxCalcMethod, v.VENDOR_ID AS VendorId, vba.BANK_ACCOUNT_NO AS BankAccNo, vba.BANK_ACCOUNT_NAME AS BankAccName, rb.BANK_CODE As BankCode, rb.BANK_NAME AS BankName",
      "from": "FROM dbo.VENDOR v WITH(NOLOCK) JOIN dbo.VENDOR_ADDR va WITH(NOLOCK) ON v.VENDOR_ID = va.VENDOR_ID JOIN dbo.VENDOR_BANK_ACC vba WITH(NOLOCK) ON v.VENDOR_ID = vba.VENDOR_ID JOIN dbo.REF_BANK rb WITH(NOLOCK) ON rb.REF_BANK_ID = vba.REF_BANK_ID",
      "where":"WHERE v.IS_ACTIVE = 1"
    },
