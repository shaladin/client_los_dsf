  "querystring": {
    "select": "SELECT A.FULL_ASSET_CODE AS FullAssetCode, A.FULL_ASSET_NAME AS FullAssetName, B.ASSET_TYPE_ID AS AssetTypeId, B.ASSET_TYPE_CODE AS AssetTypeCode, C.ASSET_CATEGORY_CODE AS AssetCategoryCode",
    "from": "FROM dbo.ASSET_MASTER A WITH(NOLOCK) LEFT JOIN dbo.ASSET_TYPE B WITH(NOLOCK) ON A.ASSET_TYPE_ID = B.ASSET_TYPE_ID LEFT JOIN dbo.ASSET_CATEGORY C WITH (NOLOCK) ON A.ASSET_CATEGORY_ID = C.ASSET_CATEGORY_ID"
  },
