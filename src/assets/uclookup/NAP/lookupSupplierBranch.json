    "querystring": {
      "select": "SELECT VND.VENDOR_CODE AS VendorCode, VND.VENDOR_NAME AS VendorName, VND.VENDOR_ID as VendorId",
      "from": "FROM dbo.VENDOR VND WITH(NOLOCK) JOIN dbo.VENDOR_OFFICE_MBR VOM WITH(NOLOCK) ON VOM.VENDOR_ID = VND.VENDOR_ID",
      "where": "WHERE VND.MR_VENDOR_CATEGORY_CODE = 'SUPPLIER_BRANCH' AND VND.IS_ACTIVE = 1 AND VOM.IS_ACTIVE = 1"
    },
