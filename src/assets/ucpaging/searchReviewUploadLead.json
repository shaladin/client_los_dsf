  "querystring": {
    "select": "SELECT RO.OFFICE_NAME AS OfficeName, UMH.UPLOAD_MONITORING_NO AS UploadNo, UMH.UPLOAD_MONITORING_H_ID AS UploadMonitoringHId, UMH.FILE_NAME AS FileName, UMH.UPLOAD_DT AS UploadDt, UMH.UPLOAD_STATUS AS UploadStatusCode, UMH.TOTAL_RECORD AS TotalRecord,  UMH.TOTAL_RECORD_PROCESSED AS TotalRecordProcessed, ISNULL(VWF.WF_TASK_LIST_ID, 0) AS TaskListId, VWF.CLAIM_STAT AS ClaimStat, VWF.USERNAME AS ClaimBy",
    "from": "FROM dbo.UPLOAD_MONITORING_H UMH WITH (NOLOCK) JOIN dbo.REF_STATUS RS WITH (NOLOCK) ON RS.REF_STATUS_CODE = UMH.UPLOAD_STATUS AND RS.STATUS_GRP_CODE = 'UPLOAD_STATUS'  JOIN dbo.REF_OFFICE AS RO WITH (NOLOCK) ON UMH.OFFICE_CODE = RO.OFFICE_CODE JOIN dbo.V_WF_TASK_LIST VWF WITH (NOLOCK) ON UMH.UPLOAD_MONITORING_NO = VWF.TRANSACTION_CODE AND VWF.ACT_CODE = 'UPLOAD_LEAD_REVIEW' AND VWF.END_EXECUTING_DATE IS NULL",
    "where": "WHERE UMH.UPLOAD_STATUS = 'RVW'"
  },
