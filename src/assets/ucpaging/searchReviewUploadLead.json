    "querystring": {
      "select": "SELECT UL.UPLOAD_MONITORING_NO AS UploadNo, RO.OFFICE_NAME AS OfficeName, STAT.DESCR AS UploadStatusCode, UL.CUST_NO AS CustNo, UL.CUST_NAME AS CustName, UL.TAX_ID_NO AS TaxIdNo, ISNULL(VWF.WF_TASK_LIST_ID, 0) AS TaskListId, VWF.CLAIM_STAT AS ClaimStat, VWF.USERNAME AS ClaimBy",
      "from": "FROM dbo.UPLOAD_LEAD AS UL WITH(NOLOCK) INNER JOIN dbo.V_REF_OFFICE AS RO WITH(NOLOCK) ON UL.ORI_OFFICE_CODE = RO.OFFICE_CODE INNER JOIN dbo.V_WF_TASK_LIST AS VWF WITH(NOLOCK) ON UL.UPLOAD_MONITORING_NO = VWF.TRANSACTION_CODE AND VWF.ACT_CODE = 'UPLOAD_LEAD_REVIEW' AND VWF.END_EXECUTING_DATE IS NULL LEFT JOIN dbo.V_REF_STATUS AS STAT WITH(NOLOCK) ON UL.UPLOAD_STATUS = STAT.REF_STATUS_CODE AND STATUS_GRP_CODE = 'UPLOAD_STATUS'",
      "where": "WHERE UL.UPLOAD_STATUS = 'RVW'"
    },
