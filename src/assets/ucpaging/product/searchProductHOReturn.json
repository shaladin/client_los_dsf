{
    "title": "Product HO Return",
    "exportExcel": false,
    "component": [
      {
        "type": "textbox",
        "name": "Prod_Code",
        "id": "ProdCodeId",
        "value": "",
        "label": "Product Code",
        "placeholder": "Placeholder"
      },
      {
        "type": "textbox",
        "name": "Prod_Name",
        "id": "ProdNameId",
        "value": "",
        "label": "Product Name",
        "placeholder": "Placeholder"
      },
      {
        "type": "textbox",
        "name": "A.PROD_DESCR",
        "id": "ProdDescId",
        "value": "",
        "label": "Product Description",
        "placeholder": "Placeholder"
      }
    ],
    "headerList": [
      {
        "type": "sort",
        "position": "center",
        "label": "Product Code",
        "name": "ProdCode"
      },
      {
        "type": "sort",
        "position": "center",
        "label": "Product Name",
        "name": "ProdName"
      },
      {
        "type": "sort",
        "position": "center",
        "label": "Product Description",
        "name": "A.PROD_DESCR"
      },
      {
        "type": "sort",
        "position": "center",
        "label": "Current Version",
        "name": "CurrentProdVersion"
      },
      {
        "type": "sort",
        "position": "center",
        "label": "Draft Version",
        "name": "DraftProdVersion"
      },
      {
        "type": "sort",
        "position": "center",
        "label": "Product Status",
        "name": "ProdStatDesc"
      },
      {
        "type": "label",
        "position": "center",
        "label": "Action"
      }
    ],
    "querystring": {
      "name": "searchProductHOReturn"
    },
    "bodyList": [
      {
        "type": "text",
        "position": "center",
        "property": "ProdCode"
      },
      {
        "type": "text",
        "position": "center",
        "property": "ProdName"
      },
      {
        "type": "text",
        "position": "center",
        "property": "Descr"
      },
      {
        "type": "link",
        "position": "center",
        "property": "CurrentProdVersion",
        "path": "VIEW_PRODUCT_HO",
        "target": "_blank",
        "param": [
          {
            "type": "prodHId",
            "property": "prodHId"
          }
        ]
      },
      {
        "type": "link",
        "position": "center",
        "property": "DraftProdVersion",
        "path": "VIEW_PRODUCT_HO",
        "target": "_blank",
        "param": [
          {
            "type": "prodHId",
            "property": "DraftProdHId"
          }
        ]
      },
      {
        "type": "text",
        "position": "center",
        "property": "ProdStatDesc"
      },
      {
  
        "type": "action",
        "position": "center",
        "action": [
          {
            "type": "switch",
            "case": [
              {
                "conditions": [
                  {
                    "property": "ProdStat",
                    "value": "DEACT",
                    "restriction": "NEQ"
                  }
                ],
                "result": {
                  "type": "callback",
                  "key": "EditProductOffering"
                }
              }
            ]
          }
        ]
      }
    ]
  }
  