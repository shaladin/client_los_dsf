{
    "title": "MOU Approval",
    "exportExcel": false,
    "component": [
        {
            "type": "textbox",
            "name": "TRX_NO",
            "id": "TrxNoId",
            "value": "",
            "label": "Transaction No",
            "placeholder": "Placeholder"
        },
        {
            "type": "textbox",
            "name": "CUST_NAME",
            "id": "CustNameId",
            "value": "",
            "label": "Customer Name",
            "placeholder": "Placeholder"
        },
        {
            "type": "textbox",
            "name": "APV_NO",
            "id": "ApvNoId",
            "value": "",
            "label": "Approval No",
            "placeholder": "Placeholder"
        },
        {
            "type": "textbox",
            "name": "TL.REQUEST_BY",
            "id": "ReqById",
            "value": "",
            "label": "Request By",
            "placeholder": "Placeholder"
        },
        {
            "type": "textbox",
            "name": "TL.CURRENT_USER_ID",
            "id": "TaskOwnerId",
            "value": "",
            "label": "Task Owner",
            "placeholder": "Placeholder"
        },
        {
            "type": "textbox",
            "name": "TL.CURRENT_USER_ID",
            "id": "CurrentUserId",
            "value": "",
            "label": "Current User",
            "placeholder": "Placeholder"
        },
        {
            "type": "datepicker",
            "name": "TL.REQUEST_DATE",
            "id": "ReqDtFromId",
            "value": "",
            "label": "Request Date From",
            "restriction": "gte",
            "datatype": "date"
        },
        {
            "type": "datepicker",
            "name": "TL.REQUEST_DATE",
            "id": "ReqDtFromId",
            "value": "",
            "label": "Request Date To",
            "restriction": "lte",
            "datatype": "date"
        }
    ],
    "querystring": {
        "select": "SELECT TL.TASK_ID AS TaskId, TL.INSTANCE_ID AS InstanceId, TL.APV_REQ_ID AS ApvReqId, TL.TRX_NO AS TrxNo, TL.APV_NO AS ApvNo, TL.REQUEST_BY AS RequestBy, TL.REQUEST_DATE AS RequestDate, TL.MAIN_USER_ID AS MainUserId, TL.CURRENT_USER_ID AS CurrentUserId, TL.CATEGORY_CODE AS CategoryCode, A.MOU_CUST_ID AS MouCustId, A.MOU_CUST_NO AS MouCustNo, A.MOU_CUST_ID AS MouCustId, A.CUST_NAME AS CustName, A.MOU_CUST_DT AS MouCustDt, A.END_DT AS EndDt, A.PLAFOND_AMT AS PlafondAmt, A.IS_REVOLVING AS IsRevolving, A.MR_MOU_TYPE_CODE AS MouType, A.MOU_STAT AS MouStat, A.REF_NO AS RefNo",
        "from": "FROM dbo.V_APV_TASK_LIST AS TL WITH(NOLOCK) JOIN dbo.MOU_CUST A WITH(NOLOCK) ON TL.TRX_NO = A.MOU_CUST_NO JOIN [dbo].[V_WF_TASK_LIST] WTL WITH(NOLOCK) ON A.MOU_CUST_NO = WTL.TRANSACTION_CODE AND WTL.ACT_CODE = N'MOU_APV'",
        "where" : "WHERE WTL.END_EXECUTING_DATE IS NULL"
    },
    "headerList": [
        {
            "type": "sort",
            "position": "center",
            "label": "APV NO",
            "name": "APV_NO"
        },
        {
            "type": "sort",
            "position": "left",
            "label": "CUSTOMER NAME",
            "name": "CustName"
        },
        {
            "type": "sort",
            "position": "center",
            "label": "MOU NO",
            "name": "MouCustNo"
        },
        {
            "type": "sort",
            "position": "left",
            "label": "REQUEST BY",
            "name": "RequestBy"
        },
        {
            "type": "sort",
            "position": "left",
            "label": "TASK OWNER",
            "name": "MainUserId"
        },
        {
            "type": "sort",
            "position": "left",
            "label": "CURRENT USER",
            "name": "CurrentUserId"
        },
        {
            "type": "sort",
            "position": "center",
            "label": "REQUEST DATE",
            "name": "RequestDate"
        },
        {
            "type": "label",
            "position": "center",
            "label": "ACTION"
        }
    ],
    "bodyList": [
        {
            "type": "text",
            "position": "center",
            "property": "TrxNo"
        },
        {
            "type": "text",
            "position": "left",
            "property": "CustName"
        },
        {
            "type": "link",
            "position": "center",
            "property": "MouCustNo",
            "path": "/Mou/Cust/View",
            "target": "_blank",
            "param": [
                {
                    "type": "MouCustId",
                    "property": "MouCustId"
                }
            ]
        },
        {
            "type": "text",
            "position": "left",
            "property": "RequestBy"
        },
        {
            "type": "text",
            "position": "left",
            "property": "MainUserId"
        },
        {
            "type": "text",
            "position": "left",
            "property": "CurrentUserId"
        },
        {
            "type": "date",
            "position": "center",
            "property": "RequestDate"
        },
        {
            "type": "action",
            "position": "center",
            "action": [
                {
                    "type": "switch",
                    "case": [
                        {
                            "conditions": [
                                {
                                    "property": "MouType",
                                    "value": "FACTORING",
                                    "restriction": "EQ"
                                }
                            ],
                            "result": {
                                "type": "url",
                                "path": "/Mou/Cust/ApprovalFactoring",
                                "icon": "ft-edit-2",
                                "param": [                                    
                                    {
                                        "type": "MouCustId",
                                        "property": "MouCustId"
                                    },
                                    {
                                        "type": "TaskId",
                                        "property": "TaskId"
                                    },
                                    {
                                        "type": "InstanceId",
                                        "property": "InstanceId"
                                    }
                                ]
                            },
                            "isHide": false
                        },
                        {
                            "conditions": [
                                {
                                    "property": "MouType",
                                    "value": "GENERAL",
                                    "restriction": "EQ"
                                }
                            ],
                            "result": {
                                "type": "url",
                                "path": "/Mou/Cust/ApprovalGeneral",
                                "icon": "ft-edit-2",
                                "param": [
                                    {
                                        "type": "MouCustId",
                                        "property": "MouCustId"
                                    },
                                    {
                                        "type": "TaskId",
                                        "property": "TaskId"
                                    },
                                    {
                                        "type": "InstanceId",
                                        "property": "InstanceId"
                                    }
                                ]
                            },
                            "isHide": false
                        }
                    ]
                }
            ]
        }
    ]
}