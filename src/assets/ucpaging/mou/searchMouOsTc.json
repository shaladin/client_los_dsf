    "querystring": {
        "select": "SELECT MOU.MOU_CUST_NO AS MouCustNo, MOU.MOU_CUST_ID AS MouCustId, MOU.CUST_NAME AS CustName, MOU.MOU_CUST_DT AS MouCustDt, MOU.START_DT AS StartDt, MOU.END_DT AS EndDt, MOU.PLAFOND_AMT PlafondAmt, MOU.IS_REVOLVING AS IsRevolving, MOU.REF_NO AS RefNo, MOU.MR_MOU_TYPE_CODE AS MrMouTypeCode",
        "from": "FROM dbo.MOU_CUST MOU WITH(NOLOCK)",
        "where": "WHERE MOU.MOU_STAT NOT IN ('NEW','RJC','CAN','EXP','ACT')"
    },
