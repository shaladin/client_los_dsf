  "querystring": {
    "select": "SELECT A.APP_NO AS AppNo, B.AGRMNT_NO AS AgrmntNo, C.CUST_NO AS CustNo, C.CUST_NAME AS CustName, A.PROD_OFFERING_NAME AS ProdOfferingName, A.PROD_OFFERING_CODE AS ProdOfferingCode, A.PROD_OFFERING_VERSION as ProdOfferingVersion, A.APP_CREATED_DT AS AppDt, A.ORI_OFFICE_CODE AS OriOfficeCode, E.INST_AMT AS InstAmt, A.IS_APP_INIT_DONE AS IsNapSub, B.AGRMNT_STAT AS ContractStat, A.APP_CURR_STEP AS AppStep, A.APP_STAT AS AppStat, A.APP_ID AS AppId,A.BIZ_TEMPLATE_CODE as BizTemplateCode, B.AGRMNT_ID as AgrmntId, C.APP_CUST_ID as AppCustId",
    "from": "FROM dbo.APP A WITH(NOLOCK) LEFT JOIN dbo.AGRMNT B WITH(NOLOCK) ON A.APP_ID = B.APP_ID LEFT JOIN dbo.APP_CUST C WITH (NOLOCK) ON A.APP_ID = C.APP_ID LEFT JOIN dbo.APP_FIN_DATA E WITH (NOLOCK) ON A.APP_ID = E.APP_ID"
  },
