                    },
                    {
                      "type": "WfTaskListId",
                      "property": "WfTaskListId"
                    }
                  ]
                },
                "isHide": false
              },
              {
                "conditions": [
                  {
                    "isUser": false,
                    "property": "ClaimBy",
                    "value": "",
                    "restriction": "EQ"
                  }
                ],
                "result": {
                  "type": "url",
                  "path": "../Detail",
                  "icon": "ft-edit-2",
