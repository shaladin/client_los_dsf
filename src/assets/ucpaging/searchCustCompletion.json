    "querystring": {
      "select": "SELECT A.ORI_OFFICE_NAME AS OfficeName, A.ORI_OFFICE_CODE AS OfficeCode, A.APP_NO AS AppNo, AC.CUST_NO AS CustNo, AC.CUST_NAME AS CustName, A.PROD_OFFERING_NAME AS ProdOfferingName, A.APP_ID AS AppId, A.PROD_OFFERING_CODE AS prodOfferingCode, A.PROD_OFFERING_VERSION AS prodOfferingVersion",
      "from": "FROM dbo.APP A WITH(NOLOCK) LEFT JOIN dbo.APP_CUST AC WITH(NOLOCK) ON A.APP_ID = AC.APP_ID",
      "where": "WHERE AC.IS_CUSTOMER = 1"
    },
    "orderby": {
        "key": "A.APP_NO", 
        "value": "false"
    },
