    ],
    "querystring": {
      "select": "SELECT A.ORI_OFFICE_NAME AS OfficeName, A.ORI_OFFICE_CODE AS OfficeCode, A.APP_NO AS AppNo, AC.CUST_NO AS CustNo, A.PROD_OFFERING_NAME AS ProdOfferingName, A.APP_ID AS AppId, WTL.USERNAME AS ClaimBy, WTL.WF_TASK_LIST_ID AS WfTaskListId, VRO.IS_ALLOW_APP_CREATED AS IsAllowAppCreated, A.PROD_OFFERING_CODE AS prodOfferingCode, A.PROD_OFFERING_VERSION AS prodOfferingVersion, WTL.SLA_COLOR AS SlaColor",
      "from": "FROM dbo.APP A WITH(NOLOCK) LEFT JOIN dbo.APP_CUST AC WITH(NOLOCK) ON A.APP_ID = AC.APP_ID JOIN [dbo].[V_WF_TASK_LIST] WTL WITH(NOLOCK) ON A.APP_NO = WTL.TRANSACTION_CODE JOIN [dbo].[V_REF_OFFICE] VRO WITH(NOLOCK) ON A.ORI_OFFICE_CODE = VRO.OFFICE_CODE",
      "where": "WHERE WTL.END_EXECUTING_DATE IS NULL AND A.IS_APP_INIT_DONE = 1"
    },
    "orderby": {
        "key": "A.APP_NO", 
        "value": "false"
    },
    "bodyList": [
      {
        "type": "sla",
        "position": "center",
        "property": "SlaColor"
      },
      {
        "type": "text",
        "position": "left",
