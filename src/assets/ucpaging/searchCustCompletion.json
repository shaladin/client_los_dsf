    "querystring": {
      "select": "SELECT A.ORI_OFFICE_NAME AS OfficeName, A.ORI_OFFICE_CODE AS OfficeCode, A.APP_NO AS AppNo, AC.CUST_NO AS CustNo, AC.CUST_NAME AS CustName, A.PROD_OFFERING_NAME AS ProdOfferingName, A.APP_ID AS AppId, A.PROD_OFFERING_CODE AS prodOfferingCode, A.PROD_OFFERING_VERSION AS prodOfferingVersion, WTL.USERNAME AS ClaimBy, WTL.WF_TASK_LIST_ID AS WfTaskListId, vro.IS_ALLOW_APP_CREATED AS IsAllowAppCreated, WTL.SLA_COLOR AS SlaColor",
      "from": "FROM dbo.APP A WITH(NOLOCK) JOIN dbo.APP_CUST AC WITH(NOLOCK) ON A.APP_ID = AC.APP_ID JOIN [dbo].[V_WF_TASK_LIST] WTL WITH (NOLOCK) ON a.APP_NO = WTL.TRANSACTION_CODE JOIN [dbo].[V_REF_OFFICE] vro WITH (NOLOCK) ON a.ORI_OFFICE_CODE = vro.OFFICE_CODE JOIN dbo.V_REF_LOB RL WITH (NOLOCK) ON a.LOB_CODE = RL.LOB_CODE",
      "where": "WHERE AC.IS_CUSTOMER = 1 AND WTL.END_EXECUTING_DATE IS NULL "
    },
    "orderby": {
        "key": "A.APP_NO", 
        "value": "false"
    },
