    "querystring": {
        "select": "SELECT vwtl.USERNAME AS ClaimBy, a.ORI_OFFICE_NAME AS OfficeName, a.ORI_OFFICE_CODE AS OfficeCode, a.APP_NO AS AppNo, ac.CUST_NAME AS CustName, a.PROD_OFFERING_NAME AS ProdOfferingName, a.APP_ID AS AppId, vwtl.CLAIM_STAT AS ClaimStat",
        "from": "FROM dbo.APP a WITH(NOLOCK) LEFT JOIN dbo.APP_CUST ac WITH(NOLOCK) ON a.APP_ID = ac.APP_ID JOIN dbo.V_WF_TASK_LIST vwtl WITH(NOLOCK) ON a.APP_NO = vwtl.TRANSACTION_CODE",
        "where": "WHERE vwtl.WF_CODE = 'RTN_EDIT_APP'"
    },
