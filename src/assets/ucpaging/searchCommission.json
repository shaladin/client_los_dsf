        "type": "textbox",
        "name": "A.APP_NO",
        "id": "AppNoId",
        "value": "",
        "label": "Application No",
        "placeholder": "Placeholder"
      },
      {
        "type": "textbox",
        "name": "AC.CUST_NAME",
        "id": "CustNameId",
        "value": "",
        "label": "Customer Name",
        "placeholder": "Placeholder"
      },
      {
        "type": "textbox",
        "name": "A.PROD_OFFERING_NAME",
        "id": "ProdOfferingNameId",
        "value": "",
        "label": "Product Offering Name",
        "placeholder": "Placeholder"
      }
    ],
    "headerList": [
      {
        "type": "sort",
        "position": "center",
        "label": "Application No",
        "name": "A.APP_NO"
      },
      {
        "type": "sort",
        "position": "center",
        "label": "Customer Name",
        "name": "AC.CUST_NAME"
      },
      {
        "type": "sort",
        "position": "center",
        "label": "Product Offering Name",
        "name": "A.PROD_OFFERING_NAME"
      },
      {
        "type": "label",
        "position": "center",
        "label": "Action"
      }
    ],
    "querystring": {
      "select": "SELECT A.APP_NO AS AppNo, AC.CUST_NAME AS CustName, A.PROD_OFFERING_NAME AS ProdOfferingName, A.APP_CURR_STEP AS AppCurrStep, A.LOB_CODE AS LobCode, A.APP_ID AS AppId",
      "from": "FROM dbo.APP A WITH(NOLOCK) LEFT JOIN dbo.APP_CUST AC WITH(NOLOCK) ON A.APP_ID = AC.APP_ID"
    },
    "bodyList": [
        {
            "type": "text",
            "position": "center",
            "property": "AppNo"
        },
        {
            "type": "text",
            "position": "center",
            "property": "CustName"
        },
        {
            "type": "text",
            "position": "center",
            "property": "ProdOfferingName"
        },
        {
            "type": "action",
            "position": "center",
            "action": [
                {
                    "type": "edit",
                    "path": "../Detail",
                    "param": [
                        {
                            "type": "AppId",
                            "property": "AppId"
                        }
                    ]
                }
            ]
          }
    ]    
}
  