{
    "title": "Document Printing",
    "exportExcel": false,
    "component": [
        {
            "type": "textbox",
            "name": "A.APP_NO",
            "id": "ApplicationNoId",
            "value": "",
            "label": "Application Number",
            "placeholder": "Placeholder"
        },
        {
            "type": "textbox",
            "name": "B.AGRMNT_NO",
            "id": "AggrementNoId",
            "value": "",
            "label": "Aggrement No",
            "placeholder": "Placeholder"
        },
        {
            "type": "textbox",
            "name": "B.CUST_NAME",
            "id": "CustNameId",
            "value": "",
            "label": "Customer Name",
            "placeholder": "Placeholder"
        },
        {
            "type": "textbox",
            "name": "B.PROD_OFFERING_NAME",
            "id": "ProdNameId",
            "value": "",
            "label": "Product Offering Name",
            "placeholder": "Placeholder"
        }
    ],
    "querystring": {
        "select":"SELECT A.APP_NO AS ApplicationNo,B.AGRMNT_NO AS AggrementNo,B.CUST_NAME AS CustName,B.PROD_OFFERING_NAME AS ProdName,B.AGRMNT_STAT AS Status, B.AGRMNT_ID AS AgrmntId, A.APP_ID AS AppId, A.BIZ_TEMPLATE_CODE AS BizTemplateCode, A.PROD_OFFERING_CODE AS ProdOfferingCode, A.PROD_OFFERING_VERSION AS ProdOfferingVersion",
        "from":"FROM dbo.APP A WITH (NOLOCK) JOIN dbo.AGRMNT B ON A.APP_ID = B.APP_ID"
    },
    "headerList": [
        {
            "type": "sort",
            "position": "center",
            "label": "Application No",
            "name": "ApplicationNo"
        },
        {
            "type": "sort",
            "position": "center",
            "label": "Aggrement No",
            "name": "AggrementNo"
        },
        {
            "type": "sort",
            "position": "left",
            "label": "Customer Name",
            "name": "CustName"
        },
        {
            "type": "sort",
            "position": "left",
            "label": "Product  Offering Name",
            "name": "ProdName"
        },
        {
            "type": "label",
            "position": "center",
            "label": "Action"
        }
    ],
    "bodyList": [
        {
            "type": "link",
            "position": "center",
            "property": "ApplicationNo",
            "target" : "_blank",
            "path": "/View/AppView",
            "param": [
                {
                "type": "AppId",
                "property": "AppId"
                }
            ]
        },
        {
            "type": "action",
            "position": "center",
            "action": [
                {
                "type": "switch",
                "case": [
                    {
                    "conditions": [
                        {
                        "property": "BizTemplateCode",
                        "value": "CF4W",
                        "restriction": "EQ"
                        }
                    ],
                    "result": {
                        "type": "link",
                        "position": "center",
                        "property": "AggrementNo",
                        "target": "_blank",
                        "path": "/View/AgrmntView",
                        "param": [
                        {
                            "type": "AgrmntId",
                            "property": "AgrmntId"
                        }
                        ]
                    },
                    "isHide": false
                    },
                    {
                    "conditions": [
                        {
                        "property": "BizTemplateCode",
                        "value": "FL4W",
                        "restriction": "EQ"
                        }
                    ],
                    "result": {
                        "type": "link",
                        "position": "center",
                        "property": "AggrementNo",
                        "target" : "_blank",
                        "path": "/View/AgrmntView",
                        "param": [
                        {
                            "type": "AgrmntId",
                            "property": "AgrmntId"
                        }
                        ]
                    },
                    "isHide": false
                    },
                    {
                    "conditions": [
                        {
                        "property": "BizTemplateCode",
                        "value": "CFRFN4W",
                        "restriction": "EQ"
                        }
                    ],
                    "result": {
                        "type": "link",
                        "position": "center",
                        "property": "AggrementNo",
                        "target" : "_blank",
                        "path": "/View/AgrmntView",
                        "param": [
                        {
                            "type": "AgrmntId",
                            "property": "AgrmntId"
                        }
                        ]
                    },
                    "isHide": false
                    },
                    {
                    "conditions": [
                        {
                        "property": "BizTemplateCode",
                        "value": "FACTORING",
                        "restriction": "EQ"
                        }
                    ],
                    "result": {
                        "type": "link",
                        "position": "center",
                        "property": "AggrementNo",
                        "target" : "_blank",
                        "path": "/View/AgrmntView",
                        "param": [
                        {
                            "type": "AgrmntId",
                            "property": "AgrmntId"
                        }
                        ]
                    },
                    "isHide": false
                    },
                    {
                        "conditions": [
                            {
                            "property": "BizTemplateCode",
                            "value": "CFNA",
                            "restriction": "EQ"
                            }
                        ],
                        "result": {
                            "type": "link",
                            "position": "center",
                            "property": "AggrementNo",
                            "target": "_blank",
                            "path": "/View/AgrmntView",
                            "param": [
                            {
                                "type": "AgrmntId",
                                "property": "AgrmntId"
                            }
                            ]
                        },
                        "isHide": false
                        }
                ]
                }
            ]
        },
        {
            "type": "text",
            "position": "left",
            "property": "CustName"
        },
        {
            "type": "callback",
            "key": "ViewProdOffering",
            "position": "left",
            "property": "ProdName"
        },
        {
            "type": "action",
            "position": "center",
            "action": [
                {
                    "type": "edit",
                    "path": "/Nap/AdminProcess/DocPrint/View",
                    "param": [
                        {
                            "type": "AgrmntId",
                            "property": "AgrmntId"
                        },
                        {
                            "type": "BizTemplateCode",
                            "property": "BizTemplateCode"
                        }
                    ]
                }
            ]
        }
    ]
}