  "querystring": {
    "select": "SELECT WTL.USERNAME AS ClaimBy, A.ORI_OFFICE_NAME AS OriOfficeName, A.APP_NO AS AppNo, AC.CUST_NAME AS CustName, A.PROD_OFFERING_NAME AS ProdOfferingName, RHD.RETURN_HANDLING_D_ID AS ReturnHandlingDId, WTL.WF_TASK_LIST_ID AS WfTaskListId, A.APP_ID AS AppId, RHD.RETURN_STAT as ReturnStat",
    "from": "FROM dbo.APP A WITH(NOLOCK) JOIN dbo.V_WF_TASK_LIST WTL WITH(NOLOCK) ON A.APP_NO = WTL.TRANSACTION_CODE JOIN dbo.RETURN_HANDLING_H RHH WITH(NOLOCK) ON A.APP_ID = RHH.APP_ID JOIN dbo.APP_CUST AC WITH(NOLOCK) ON A.APP_ID = AC.APP_ID JOIN dbo.RETURN_HANDLING_D RHD WITH(NOLOCK) ON RHH.RETURN_HANDLING_H_ID = RHD.RETURN_HANDLING_H_ID",
    "where": "WHERE WTL.ACT_CODE = 'RTN_ADD_COLTR' AND RHD.MR_RETURN_TASK_CODE = 'RTN_ADD_COLTR' AND RHD.RETURN_STAT = 'REQ'"
  },
