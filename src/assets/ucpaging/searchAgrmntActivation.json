    "querystring": {
        "select": "SELECT A.APP_NO,AC.CUST_NAME as CustName, A.PROD_OFFERING_NAME as ProdOfferingName, WF.USERNAME as ClaimBy, WF.WF_TASK_LIST_ID as WFTaskListId, WF.TRANSACTION_CODE as TrxNo",
        "from": "FROM APP A WITH(NOLOCK) JOIN APP_CUST AC ON A.APP_ID = AC.APP_ID JOIN V_WF_TASK_LIST WF WITH(NOLOCK) ON A.APP_NO = WF.TRANSACTION_CODE"
    },
