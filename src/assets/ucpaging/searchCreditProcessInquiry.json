  "querystring": {
    "select": "SELECT A.APP_ID AS AppId, A.APP_NO AS AppNo, AC.CUST_NO AS CustNo,AGR.AGRMNT_NO AS AgrmntNo,AGR.AGRMNT_ID AS agrmntId, AC.CUST_NAME AS CustName, AGR.AGRMNT_STAT AS AgrmntStat, AC.CUST_NAME AS CustName, A.APP_CREATED_DT AS AppCreatedDt, A.IS_APP_INIT_DONE AS isNapSub, ISNULL(AGR.AGRMNT_LAST_STEP,A.APP_LAST_STEP) AS AppLastStep, ISNULL(AGR.AGRMNT_CURR_STEP,A.APP_CURR_STEP) AS AppCurrStep, DATEDIFF(SECOND, A.DTM_CRT, A.DTM_UPD) AS TotalTimeNap , CONVERT(VARCHAR(12), DATEDIFF(SECOND,A.DTM_CRT,A.DTM_UPD) / 60 / 60 / 24) + ' Day(s) ' +  CONVERT(VARCHAR(12), DATEDIFF(SECOND,A.DTM_CRT,A.DTM_UPD) / 60 / 60 % 24) + ' Hour(s) ' +  CONVERT(VARCHAR(2), DATEDIFF(SECOND,A.DTM_CRT,A.DTM_UPD) / 60 % 60) + ' Minute(s) ' +  CONVERT(VARCHAR(2), DATEDIFF(SECOND,A.DTM_CRT,A.DTM_UPD) % 60) + ' Second(s) ' AS 'DateDiff', RE.EMP_NAME AS SalesOfficerName, AC.APP_CUST_ID AS AppCustId",
    "from": "FROM dbo.APP A WITH(NOLOCK) LEFT JOIN dbo.AGRMNT AGR WITH(NOLOCK) ON A.APP_ID = AGR.APP_ID JOIN dbo.APP_CUST AC WITH(NOLOCK) ON AC.APP_ID = A.APP_ID AND AC.IS_CUSTOMER=1 JOIN dbo.APP_FIN_DATA FD WITH(NOLOCK) ON FD.APP_ID = A.APP_ID JOIN [dbo].[V_EMP_NAME] RE WITH (NOLOCK) ON AGR.SALES_OFFICER_NO = RE.EMP_NO"
  },
