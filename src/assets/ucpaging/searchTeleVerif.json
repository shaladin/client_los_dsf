  "querystring": {
    "select": "SELECT L.LEAD_NO AS LeadNo, WTL.WF_TASK_LIST_ID AS WfTaskListId, L.ORI_OFFICE_NAME AS OfficeName,WTL.USERNAME AS ClaimBy, WTL.CLAIM_STAT AS ClaimStat, L.ORI_OFFICE_CODE AS OfficeCode, LC.CUST_NAME AS CustName,  LA.FULL_ASSET_NAME AS FullAssetName, L.ORDER_NO AS OrderNo, L.LEAD_ID AS LeadId, L.MR_LEAD_SOURCE_CODE as MrLeadSourceCode ,RM.DESCR AS LeadSource",
    "from": "FROM dbo.LEAD L WITH(NOLOCK) JOIN dbo.LEAD_CUST LC WITH(NOLOCK) ON LC.LEAD_ID = L.LEAD_ID LEFT JOIN dbo.LEAD_ASSET LA WITH(NOLOCK) ON LA.LEAD_ID = LC.LEAD_ID LEFT JOIN [V_REF_MASTER] RM ON RM.MASTER_CODE = L.MR_LEAD_SOURCE_CODE AND RM.REF_MASTER_TYPE_CODE = 'LEAD_SOURCE' JOIN [dbo].[V_WF_TASK_LIST] WTL WITH(NOLOCK) ON L.LEAD_NO = WTL.TRANSACTION_CODE AND WTL.ACT_CODE = 'TELE_VERIFICATION'",
    "where" : "WHERE WTL.END_EXECUTING_DATE IS NULL"
  },
  "bodyList": [
