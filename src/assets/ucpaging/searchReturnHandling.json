  "querystring": {
    "select": "SELECT WTL.USERNAME AS ClaimBy, A.ORI_OFFICE_NAME AS OriOfficeName, A.APP_NO AS AppNo, AC.CUST_NAME AS CustName, AC.MR_CUST_TYPE_CODE AS MrCustTypeCode, A.PROD_OFFERING_NAME AS ProdOfferingName, A.PROD_OFFERING_CODE AS ProdOfferingCode, A.PROD_OFFERING_VERSION AS ProdOfferingVersion, RHH.RETURN_HANDLING_H_ID AS ReturnHandlingHId, WTL.WF_TASK_LIST_ID AS WfTaskListId, A.APP_ID AS AppId, A.BIZ_TEMPLATE_CODE AS BizTemplateCode, VRS.DESCR AS ReturnHStatusName, WTL.SLA_COLOR AS SlaColor",
    "from": "FROM dbo.APP A WITH(NOLOCK) JOIN dbo.V_WF_TASK_LIST WTL WITH(NOLOCK) ON A.APP_NO = WTL.TRANSACTION_CODE JOIN dbo.RETURN_HANDLING_H RHH WITH(NOLOCK) ON A.APP_ID = RHH.APP_ID JOIN dbo.APP_CUST AC WITH(NOLOCK) ON A.APP_ID = AC.APP_ID LEFT JOIN V_REF_STATUS VRS WITH(NOLOCK) ON RHH.RETURN_H_STATUS = VRS.REF_STATUS_CODE AND VRS.STATUS_GRP_CODE = 'RETURN_HANDLING_H_STAT'",
    "where": "WHERE WTL.END_EXECUTING_DATE IS NULL AND RHH.RETURN_H_STATUS <> N'FIN'"
  },
