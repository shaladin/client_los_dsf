{
    "title": "Credit Approval",
    "exportExcel": false,
    "component": [
      {
        "type": "textbox",
        "name": "A.APP_NO",
        "id": "AppNoId",
        "value": "",
        "label": "Application No",
        "placeholder": "Placeholder"
      },
      {
        "type": "textbox",
        "name": "AC.CUST_NAME",
        "id": "CustNameId",
        "value": "",
        "label": "Customer Name",
        "placeholder": "Placeholder"
      },
      {
        "type": "textbox",
        "name": "A.PROD_OFFERING_NAME",
        "id": "ProdOfferingNameId",
        "value": "",
        "label": "Product Offering Name",
        "placeholder": "Placeholder"
      },
      {
        "type": "dropdown",
        "name": "a.ORI_OFFICE_CODE",
        "id": "OfficeNameId",
        "value": "Office",
        "placeholder": "Placeholder",
        "isFromURL": true,
        "isSelectOne": false,
        "url": "http://r3app-server.ad-ins.com/Foundation_R3/RefOffice/GetListKvpActiveRefOfficeForPaging",
        "label": "Office",
        "items": [],
        "itemsUrl": []
      },
      {
        "type": "textbox",
        "name": "ATL.CURRENT_USER_ID",
        "id": "claimById",
        "value": "",
        "label": "CLAIM BY",
        "placeholder": "Placeholder"
      }
    ],
    "headerList": [
      {
        "type": "sort",
        "position": "left",
        "label": "Claim By",
        "name": "ClaimBy"
      },
      {
        "type": "sort",
        "position": "left",
        "label": "Office",
        "name": "a.ORI_OFFICE_NAME"
      },
      {
        "type": "sort",
        "position": "center",
        "label": "Application No",
        "name": "a.APP_NO"
      },
      {
        "type": "sort",
        "position": "center",
        "label": "Customer Name",
        "name": "ac.CUST_NAME"
      },
      {
        "type": "sort",
        "position": "center",
        "label": "Product Offering Name",
        "name": "a.PROD_OFFERING_NAME"
      },
      {
        "type": "label",
        "position": "center",
        "label": "Action"
      }
    ],
    "querystring": {
      "select": "SELECT a.ORI_OFFICE_NAME AS OfficeName, a.ORI_OFFICE_CODE AS OfficeCode, a.APP_NO AS AppNo, ac.CUST_NAME AS CustName, ac.MR_CUST_TYPE_CODE AS MrCustTypeCode, a.PROD_OFFERING_NAME AS ProdOfferingName, a.APP_ID AS AppId, ATL.CURRENT_USER_ID AS ClaimBy, ATL.INSTANCE_ID AS InstanceId, ATL.TASK_ID AS TaskId, vro.IS_ALLOW_APP_CREATED AS IsAllowAppCreated",
      "from": "FROM dbo.APP a WITH ( NOLOCK ) LEFT JOIN dbo.APP_CUST ac WITH ( NOLOCK ) ON a.APP_ID = ac.APP_ID JOIN [dbo].[V_APV_TASK_LIST] ATL WITH ( NOLOCK ) ON a.APP_NO = ATL.TRX_NO  AND ATL.CATEGORY_CODE = 'CRD_APV' JOIN [dbo].[V_REF_LOB] RL WITH ( NOLOCK ) ON a.BIZ_TEMPLATE_CODE = RL.BIZ_TMPLT_CODE JOIN [dbo].[V_REF_OFFICE] vro WITH ( NOLOCK ) ON a.ORI_OFFICE_CODE = vro.OFFICE_CODE ",
      "where": "WHERE a.IS_APP_INIT_DONE = '1'"
    },
    "bodyList": [
      {
        "type": "text",
        "position": "left",
        "property": "ClaimBy"
      },
      {
        "type": "text",
        "position": "left",
        "property": "OfficeName"
      },
      {
        "type": "link",
        "position": "center",
        "property": "AppNo",
        "target": "_blank",
        "path": "/Nap/View/AppView",
        "param": [
          {
            "type": "AppId",
            "property": "AppId"
          }
        ]
      },
      {
        "type": "text",
        "position": "center",
        "property": "CustName"
      },
      {
        "type": "text",
        "position": "center",
        "property": "ProdOfferingName"
      },
      {
        "type": "action",
        "position": "center",
        "action": [
          {
            "type": "switch",
            "case": [
              {
                "conditions": [
                  {
                    "isUser": true,
                    "property": "ClaimBy",
                    "value": "",
                    "restriction": "EQ"
                  }
                ],
                "result": {
                  "type": "url",
                  "path": "/Nap/CreditProcess/CreditApproval/Detail",
                  "icon": "ft-edit-2",
                  "param": [
                    {
                      "type": "AppId",
                      "property": "AppId"
                    },
                    {
                      "type": "TaskId",
                      "property": "TaskId"
                    },
                    {
                      "type": "InstanceId",
                      "property": "InstanceId"
                    },
                    {
                      "type": "MrCustTypeCode",
                      "property": "MrCustTypeCode"
                    }
                  ]
                },
                "isHide": false
              },
              {
                "conditions": [
                  {
                    "isUser": false,
                    "property": "ClaimBy",
                    "value": "",
                    "restriction": "EQ"
                  }
                ],
                "result": {
                  "type": "url",
                  "path": "/Nap/CreditProcess/CreditApproval/Detail",
                  "icon": "ft-edit-2",
                  "param": [
                    {
                      "type": "AppId",
                      "property": "AppId"
                    },
                    {
                      "type": "TaskId",
                      "property": "TaskId"
                    },
                    {
                      "type": "InstanceId",
                      "property": "InstanceId"
                    },
                    {
                      "type": "MrCustTypeCode",
                      "property": "MrCustTypeCode"
                    }
                  ]
                },
                "isHide": false
              }
            ]
          }
        ]
      }
    ]
  }
  