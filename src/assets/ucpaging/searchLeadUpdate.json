    "querystring": {
        "select": "SELECT L.LEAD_NO AS LeadNo, L.LEAD_ID AS LeadId, L.CRT_OFFICE_NAME AS CrtOfficeName, L.ORI_OFFICE_NAME AS OriOfficeName, L.ORDER_NO AS OrderNo, LC.CUST_NAME AS CustName, L.MR_LEAD_SOURCE_CODE AS SourceCode, LA.FULL_ASSET_NAME AS FullAssetName , WTL.USERNAME AS ClaimBy, WTL.CLAIM_STAT AS ClaimStat, WTL.WF_TASK_LIST_ID AS TaskListId",
        "from": "FROM LEAD L WITH(NOLOCK) JOIN LEAD_ASSET LA WITH(NOLOCK) ON L.LEAD_ID = LA.LEAD_ID JOIN LEAD_CUST LC WITH(NOLOCK) ON L.LEAD_ID = LC.LEAD_ID LEFT JOIN [dbo].[V_WF_TASK_LIST] WTL WITH(NOLOCK) ON L.LEAD_NO = WTL.TRANSACTION_CODE AND WTL.ACT_CODE = 'LEAD_UPDATE'"
    },
