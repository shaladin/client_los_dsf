      "name": "AGR.CUST_NAME"
    },
    {
      "type": "sort",
      "position": "left",
      "label": "PRODUCT OFFERING NAME",
      "name": "A.PROD_OFFERING_NAME"
    },
    {
      "type": "label",
      "position": "center",
      "label": "Action"
    }
  ],

  "bodyList": [
