    },
    {
      "type": "dropdown",
      "datatype": "text",
      "name": "A.ORI_OFFICE_CODE",
      "id": "OfficeNameId",
      "value": "",
      "placeholder": "Placeholder",
      "isFromURL": true,
      "isSelectOne": false,
      "isQueryIn" : true,
      "url": "",
      "path":"/RefOffice/GetListKvpActiveRefOfficeForPaging",
      "label": "OFFICE NAME",
      "items": [],
      "itemsUrl": []
    },
    {
      "type": "dropdown",
      "name": "WF.CLAIM_STAT",
      "id": "ActiveStatusId",
      "label": "TASK CLAIM STATUS",
      "value": "",
      "placeholder": "Placeholder",
      "isFromURL": false,
      "isSelectOne": false,
      "items": [
        {
          "key": "CLAIMED",
          "value": "CLAIMED"
        },
        {
          "key": "UNCLAIMED",
          "value": "UNCLAIMED"
        }
      ],
      "itemsUrl": []
    },
    {
      "type": "textbox",
      "name": "WF.USERNAME",
      "id": "TaskClaimById",
      "value": "",
      "label": "Task Claim By",
      "placeholder": "Placeholder"
    }
  ],
  "querystring": {
    "select": "SELECT  WF.USERNAME AS Username, A.ORI_OFFICE_NAME AS OfficeName, WF.WF_TASK_LIST_ID as TaskListId, AGR.AGRMNT_NO AS AgrmntNo, A.APP_NO AS AppNo, AGR.CUST_NO AS CustNo, AGR.CUST_NAME AS CustName, A.PROD_OFFERING_NAME AS ProdOfferingName, CASE WHEN WF.USERNAME = NULL OR WF.USERNAME = '' THEN 'UNCLAIMED' ELSE 'CLAIMED' END AS ClaimStatus, A.APP_ID AS AppId, AGR.AGRMNT_ID AS AgrmntId, A.BIZ_TEMPLATE_CODE AS BizTemplateCode",
    "from": "FROM dbo.APP A WITH(NOLOCK) JOIN dbo.AGRMNT AGR WITH(NOLOCK) ON AGR.APP_ID = A.APP_ID JOIN dbo.V_WF_TASK_LIST WF WITH(NOLOCK) ON WF.TRANSACTION_CODE = AGR.AGRMNT_NO ",
    "where": "WHERE WF.END_EXECUTING_DATE IS NULL"
  },
  "orderby": {
    "key": "A.APP_NO",
    "value": "false"
  },
  "headerList": [
    {
      "type": "sort",
      "position": "left",
      "label": "Claim By",
      "name": "WF.USERNAME"
    },
    {
      "type": "sort",
      "position": "left",
      "label": "OFFICE",
      "name": "A.ORI_OFFICE_NAME"
    },
    {
      "type": "sort",
      "position": "center",
