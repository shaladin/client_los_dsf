        },
        {
            "type" : "textbox",
            "name" : "WF.USERNAME",
            "id" : "TaskClaimById",
            "value" : "",
            "label" : "Task Claim By",
            "placeholder": "Placeholder"
        }
    ],
    "querystring": {
        "select": "SELECT WF.WF_TASK_LIST_ID as TaskListId, WF.USERNAME AS Username, AGR.AGRMNT_NO AS AgrmntNo, A.APP_NO AS AppNo, AGR.CUST_NO AS CustNo, AGR.CUST_NAME AS CustName, A.PROD_OFFERING_NAME AS ProdOfferingName, CASE WHEN WF.USERNAME = NULL OR WF.USERNAME = '' THEN 'UNCLAIMED' ELSE 'CLAIMED' END AS ClaimStatus, A.APP_ID AS AppId, AGR.AGRMNT_ID AS AgrmntId, A.BIZ_TEMPLATE_CODE AS BizTemplateCode",
        "from": "FROM dbo.APP A WITH(NOLOCK) JOIN dbo.AGRMNT AGR WITH(NOLOCK) ON AGR.APP_ID = A.APP_ID JOIN dbo.V_WF_TASK_LIST WF WITH(NOLOCK) ON WF.TRANSACTION_CODE = AGR.AGRMNT_NO",
        "where" : "WHERE WF.END_EXECUTING_DATE IS NULL"
    },
    "orderby": {
        "key": "A.APP_NO", 
        "value": "false"
    },
    "headerList": [
        {
