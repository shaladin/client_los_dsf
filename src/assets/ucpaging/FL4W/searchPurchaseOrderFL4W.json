        {
            "type" : "textbox",
            "name" : "WF.USERNAME",
            "id" : "TaskClaimById",
            "value" : "",
            "label" : "Task Claim By",
            "placeholder": "Placeholder"
        }
    ],
    "querystring": {
        "select": "SELECT WF.USERNAME AS Username, AGR.AGRMNT_NO AS AgrmntNo, A.APP_NO AS AppNo, AGR.CUST_NAME AS CustName, A.PROD_OFFERING_NAME AS ProdOfferingName, CASE WHEN WF.USERNAME = NULL OR WF.USERNAME = '' THEN 'UNCLAIMED' ELSE 'CLAIMED' END AS ClaimStatus, A.APP_ID AS AppId, AGR.AGRMNT_ID AS AgrmntId",
        "from": "FROM dbo.APP AS A WITH(NOLOCK) INNER JOIN dbo.AGRMNT AS AGR WITH(NOLOCK) ON AGR.APP_ID = A.APP_ID INNER JOIN dbo.V_WF_TASK_LIST AS WF WITH(NOLOCK) ON WF.TRANSACTION_CODE = A.APP_NO"
    },
    "headerList": [
        {
