    "querystring": {
        "select": "SELECT UMH.UPLOAD_MONITORING_NO AS UploadMonitoringNo, UMH.OFFICE_CODE AS OfficeCode, RO.OFFICE_NAME AS OfficeName, UL.UPLOAD_STATUS AS UploadStatus, STAT.DESCR AS UploadStatusDescr, UMH.FILE_NAME AS FileName, UMH.TOTAL_RECORD AS TotalRecord, UMH.TOTAL_RECORD_PROCESSED AS TotalRecordProcessed, UMH.TOTAL_RECORD_ERROR AS TotalRecordError, UMH.IS_EXECUTED AS IsExecuted, UL.ERROR_MESSAGE AS ErrorMessage ",
        "from": "FROM dbo.V_UPLOAD_MONITORING_H UMH WITH ( NOLOCK ) JOIN dbo.UPLOAD_LEAD AS UL WITH ( NOLOCK ) ON UMH.UPLOAD_MONITORING_NO = UL.UPLOAD_MONITORING_NO JOIN dbo.V_REF_OFFICE AS RO WITH ( NOLOCK ) ON UL.ORI_OFFICE_CODE = RO.OFFICE_CODE LEFT JOIN dbo.V_REF_STATUS AS STAT WITH ( NOLOCK ) ON UL.UPLOAD_STATUS = STAT.REF_STATUS_CODE AND STAT.STATUS_GRP_CODE = N'UPLOAD_STATUS'",
        "groupby": "GROUP BY UMH.UPLOAD_MONITORING_NO, UMH.OFFICE_CODE, RO.OFFICE_NAME, UL.UPLOAD_STATUS, STAT.DESCR, UMH.FILE_NAME, UMH.TOTAL_RECORD, UMH.TOTAL_RECORD_PROCESSED, UMH.TOTAL_RECORD_ERROR, UMH.IS_EXECUTED, UL.ERROR_MESSAGE "
    },
