        },
        {
            "type": "dropdown",
            "datatype": "text",
            "name": "WF.TASK_CLAIM_STAT",
            "id": "TaskClaimStatId",
            "value": "",
            "placeholder": "Placeholder",
            "isFromURL": true,
            "isSelectOne": true,
            "url": "http://r3app-server.ad-ins.com/FOUNDATION_R3/RefMaster/GetListKeyValueActiveByCode",
            "label": "Task Claim Status",
            "criteriaPropName": "RefMasterTypeCode",
            "criteriaPropValue": "OFFICE_CLASS",
            "items": [
                {
                    "key": "CLAIMED",
                    "value": "CLAIMED"
                },
                {
                    "key": "UNCLAIMED",
                    "value": "UNCLAIMED"
                }
            ],
            "itemsUrl": []
        }
    ],
    "querystring": {
        "select": "SELECT WF.USERNAME AS UserName, TASK_CLAIM_STAT AS TaskClaimStat, CASE WHEN WF.USERNAME = NULL OR WF.USERNAME = '' THEN 'UNCLAIMED' ELSE 'CLAIMED' END AS ClaimStatus, AG.AGRMNT_NO AS AgrmntNo, AP.APP_NO AS AppNo, AG.PROD_OFFERING_NAME AS ProdOfferingName, AG.CUST_NAME AS CustName, AG.APP_ID AS AppId, AG.AGRMNT_CURR_STEP AS AgrmntCurrStep",
        "from": "FROM dbo.APP AP WITH(NOLOCK) JOIN dbo.AGRMNT AG WITH(NOLOCK) ON AP.APP_ID = AG.APP_ID JOIN dbo.V_WF_TASK_LIST WF WITH(NOLOCK) ON WF.TRANSACTION_CODE = AP.APP_NO"
    },
    "headerList": [
