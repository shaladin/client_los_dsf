            "type" : "action",
            "position" : "center",
            "action" : [
              {
                "type": "switch",
                "case": [
                  {
                    "conditions" : [ 
                      {
                          "property" : "CurrentUser", 
                          "value" : "-", 
                          "restriction" : "NEQ" 
                      }
                    ],
                    "result" : { 
                      "type": "callback",
                      "key": "HoldTask",
                      "icon" : "ft-edit-2"
                    },
                    "isHide" : false
                  }
                ]
              }
            ]
          },
          {
            "type" : "action",
            "position" : "center",
            "action" : [
              {
                "type": "switch",
                "case": [
                  {
                    "conditions" : [ 
                      {
                          "isUser": false,
                          "property" : "IsRoleAssignment", 
                          "value" : false, 
                          "restriction" : "EQ" 
                      }
                    ],
                    "result" : { 
                      "type": "callback",
                      "key": "TakeBack",
                      "icon" : "ft-edit-2"
                    },
                    "isHide" : false
                  }
                ]
              }
            ]
          },
          {
            "type" : "action",
            "position" : "center",
            "action" : [
              {
                "type": "switch",
                "case": [
                  {
                    "conditions" : [ 
                      {
                          "isUser": false,
                          "property" : "IsRoleAssignment", 
                          "value" : true, 
                          "restriction" : "EQ" 
                      }
                    ],
                    "result" : { 
                      "type": "callback",
