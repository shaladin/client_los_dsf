            "path": "/Nap/View/AppView",
            "target" : "_blank",
            "param": [
              {
                "property": "AppId"
              }
            ]
          }, 
          {
            "type": "text",
            "label": "Number Of Asset",
            "property": "NumOfAsset"
          },
          {
            "type": "text",
            "label": "Customer Name",
            "property": "CustName"
          },
          {
            "type": "text",
            "label": "Original Office",
            "property": "OfficeName"
          },
          {
            "type": "text",
            "label": "Tenor",
            "property": "Tenor"
          },
          {
            "type": "callback",
            "key": "ViewProdOffering",
            "property": "ProdOfferingName"
          },
          {
            "type": "text",
            "label": "Curreny",
            "property": "CurrCode"
          },
          {
            "type": "text",
            "label": "Order Information",
            "property": "MrOrderInfoCode"
          }
        ],
        "querystring": {
          "select": "SELECT A.APP_NO AS AppNo,  A.APP_ID AS AppId, AC.CUST_NAME AS CustName, A.TENOR AS Tenor, A.CURR_CODE AS CurrCode, A.NUM_OF_ASSET AS NumOfAsset, A.ORI_OFFICE_NAME AS OfficeName, A.PROD_OFFERING_NAME AS ProdOfferingName, A.PROD_OFFERING_CODE AS ProdOfferingCode, A.PROD_OFFERING_VERSION AS ProdOfferingVersion, A.MR_ORDER_INFO_CODE AS MrOrderInfoCode",
          "from": "FROM APP A WITH(NOLOCK) LEFT JOIN APP_CUST AC WITH(NOLOCK) ON A.APP_ID = AC.APP_ID",
          "where": "WHERE A.APP_ID = {0}"
        }
      }
    ]
