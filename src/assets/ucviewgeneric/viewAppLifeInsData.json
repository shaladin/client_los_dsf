        "mainInfo": [
          {
            "type": "text",
            "label": "Cover Life Insurance",
            "property": "IsLifeInsCover"
          },
          {
            "type": "text",
            "label": "Life Insurance Company",
            "property": "LifeInscoBranchName"
          },
          {
            "type": "currency",
            "label": "Total Premi to Customer",
            "property": "TotPremiCust"
          },
          {
            "type": "currency",
            "label": "Total Premi from Insco",
            "property": "TotPremiInsco"
          },
          {
            "type": "currency",
            "label": "Admin Fee to Customer",
            "property": "AdminFeeCust"
          },
          {
            "type": "currency",
