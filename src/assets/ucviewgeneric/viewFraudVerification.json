            "querystring": {
                "select": "SELECT l.LEAD_NO AS LeadNo, l.ORI_OFFICE_NAME AS OriOfficeName, lc.CUST_NAME AS CustName, la .TOTAL_DOWN_PAYMENT_AMT AS TotalDownPaymentAmt, la.TENOR AS Tenor, lasset.FULL_ASSET_NAME AS FullAssetName, la.NTF_AMT AS NtfAmt",
                "from": "From dbo.LEAD_APP la WITH(NOLOCK) JOIN LEAD_CUST lc ON la.LEAD_ID = lc.LEAD_ID  JOIN LEAD l WITH(NOLOCK) ON l.LEAD_ID = lc.LEAD_ID JOIN LEAD_ASSET lasset WITH(NOLOCK) on lasset.LEAD_ID = lc.LEAD_ID ",
                "where": "WHERE lc.LEAD_ID = {0}"
            }
