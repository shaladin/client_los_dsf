            "subSectionTitle": "Main Information",
            "mainInfoUrl": "http://r3app-server.ad-ins.com/LOS/Generic/GetDataViewBySQL",
            "mainInfo": [
                {
                    "type": "link",
                    "label": "Lead No",
                    "property": "LeadNo",
                    "position" : "left",
                    "path": "/Lead/View",
                    "param": [
                      {
                        "property": "LeadId"
                      }
                    ]
                  },
                {
                    "type": "text",
                    "label": "Ori Office Name",
                    "property": "OfficeName"
                },
                {
                    "type": "text",
                    "label": "Customer Name",
                    "property": "CustName"
                },
                {
                    "type": "text",
                    "label": "Down Payment Amount / (%) DP",
                    "property": "TotalDownPaymentAmt"
                },
                {
                    "type": "text",
                    "label": "Tenor",
                    "property": "Tenor"
                },
                {
                    "type": "text",
                    "label": "Full Asset Name",
                    "property": "FullAssetName"
                },
                {
                    "type": "currency",
                    "label": "NTF Amount",
                    "property": "NtfAmt"
                }
            ],
            "querystring": {
                "select": "SELECT l.LEAD_NO AS LeadNo, l.ORI_OFFICE_CODE AS OriOfficeCode, lc.CUST_NAME AS CustName, la .TOTAL_DOWN_PAYMENT_AMT AS TotalDownPaymentAmt, la.TENOR AS Tenor, lasset.FULL_ASSET_NAME AS FullAssetName, la.NTF_AMT AS NtfAmt, RO.OFFICE_NAME AS OfficeName",
                "from": "From dbo.LEAD_APP la WITH(NOLOCK) JOIN LEAD_CUST lc ON la.LEAD_ID = lc.LEAD_ID  JOIN LEAD l WITH(NOLOCK) ON l.LEAD_ID = lc.LEAD_ID JOIN LEAD_ASSET lasset WITH(NOLOCK) on lasset.LEAD_ID = lc.LEAD_ID JOIN dbo.V_REF_OFFICE RO WITH(NOLOCK) ON RO.OFFICE_CODE = l.ORI_OFFICE_CODE",
                "where": "WHERE lc.LEAD_ID = {0}"
            }
        }
    ]
}