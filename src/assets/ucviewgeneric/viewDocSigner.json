{
    "title": "",
    "subsection": [
      {
        "subSectionId": "MainInfoId",
        "subSectionTitle": "Main Information",
        "mainInfoUrl": "http://r3app-server.ad-ins.com/LOS/Generic/GetDataViewBySQL",
        "mainInfo": [
          {
            "type": "link",
            "label": "Application No",
            "property": "AppNo",
            "target": "_blank",
            "path": "/Nap/View/AppView",
            "param": [
              {
                "type" : "AppId", 
                "property" : "AppId" 
              }
            ]
          },
          {
            "type": "text",
            "label": "Num Of Asset",
            "property": "NumOfAsset"
          },
          {
            "type" : "callback",
            "label": "Agreement No",
            "property": "AgrmntNo",
            "key": "agrmnt"
          },
          {
            "type": "text",
            "label": "Tenor",
            "property": "Tenor"
          },
          {
            "type": "text",
            "label": "Customer Name",
            "property": "CustName"
          },
          {
            "type": "text",
            "label": "Interest Type",
            "property": "MrInterestTypeCode"
          },
          {
            "type": "callback",
            "label": "Product Offering Name",
            "property": "ProdOfferingName",
            "key": "prodOff"
          },
          {
            "type": "text",
            "label": "Installment Scheme",
            "property": "MrInstSchemeCode"
          }
        ],
        "querystring": {
          "select": "SELECT A.APP_NO AS AppNo, A.APP_ID AS AppId, AG.NUM_OF_ASSET AS NumOfAsset, AG.AGRMNT_NO AS AgrmntNo, AG.AGRMNT_ID AS AgrmntId, AG.TENOR AS Tenor, AG.CUST_NAME AS CustName, AFD.MR_INTEREST_TYPE_CODE AS MrInterestTypeCode, AG.PROD_OFFERING_NAME AS ProdOfferingName, AG.PROD_OFFERING_CODE AS ProdOfferingCode, AFD.MR_INST_SCHEME_CODE AS MrInstSchemeCode",
          "from": "FROM DBO.APP A WITH(NOLOCK) JOIN DBO.AGRMNT AG WITH(NOLOCK) ON A.APP_ID = AG.APP_ID LEFT JOIN DBO.AGRMNT_FIN_DATA AFD WITH(NOLOCK) ON AG.AGRMNT_ID = AFD.AGRMNT_ID",
          "where": "WHERE A.APP_ID = {0}"
        }
      }
    ]
  }