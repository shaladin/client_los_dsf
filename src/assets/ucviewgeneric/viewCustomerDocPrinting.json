        "querystring": {
          "select": "SELECT MC.MOU_CUST_NO AS MouCustNo, MC.CUST_NAME AS CustName, MC.START_DT AS StartDt, MC.END_DT AS EndDt, MC.MOU_STAT AS MouStat, MC.MOU_CUST_DT AS MouCustDt, MC.MR_MOU_TYPE_CODE AS MrMouTypeCode, MC.PLAFOND_AMT AS PlafondAmt, MC.REF_NO AS RefNo, MC.MOU_CUST_ID AS MouCustId",
          "from": " FROM dbo.MOU_CUST MC WITH(NOLOCK) LEFT JOIN dbo.MOU_CUST_DOC_PRINT MCDP WITH(NOLOCK) ON MCDP.MOU_CUST_ID = MC.MOU_CUST_ID ",
