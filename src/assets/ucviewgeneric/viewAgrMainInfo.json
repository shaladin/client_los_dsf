          "path": "/View/AgrmntView",
          "target" : "_blank",
          "param": [
            {
              "type": "AgrmntId",
              "property": "AgrmntId"
            }
          ]
        }, 
        {
          "type": "text",
          "label": "Customer Name",
          "property": "CustName"
