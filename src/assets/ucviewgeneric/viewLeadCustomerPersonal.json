        "from": "FROM dbo.LEAD_CUST lc WITH(NOLOCK) JOIN dbo.LEAD_CUST_PERSONAL lcp WITH(NOLOCK) ON lc.LEAD_CUST_ID = lcp.LEAD_CUST_ID",
