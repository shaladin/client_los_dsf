          "property": "CurrCode"
        }
      ],
      "querystring": {
        "select": "SELECT A.APP_NO AS AppNo, AGR.AGRMNT_NO AS AgrmntNo, AGR.CUST_NAME AS CustName, vCust.CUST_ID AS CustId ,AGR.CUST_NO AS CustNo, AGR.TENOR AS Tenor, AGR.CURR_CODE AS CurrCode, AGR.NUM_OF_ASSET AS NumOfAsset, AGR.OFFICE_NAME AS OfficeName, A.PROD_OFFERING_NAME AS ProdOfferingName, A.PROD_OFFERING_CODE AS ProdOfferingCode, A.PROD_OFFERING_VERSION AS ProdOfferingVersion, A.MR_ORDER_INFO_CODE AS MrOrderInfoCode, L.LEAD_NO AS LeadNo, A.LEAD_ID AS LeadId, AGR.AGRMNT_ID AS AgrmntId, A.APP_ID AS AppId",
        "from": "FROM dbo.AGRMNT AGR WITH(NOLOCK) JOIN dbo.APP A WITH(NOLOCK) ON A.APP_ID = AGR.APP_ID JOIN dbo.APP_CUST apCust WITH(NOLOCK) ON apCust.APP_ID = A.APP_ID JOIN dbo.V_CUST vCust WITH(NOLOCK) ON agr.CUST_NO = vCust.CUST_NO LEFT JOIN dbo.LEAD L ON L.LEAD_ID = A.LEAD_ID",
        "where": "WHERE AGR.AGRMNT_ID = {0}"
      }
    }
  ]
