      "subSectionId": "ViewLeadLegalAddressId",
      "subSectionTitle": "Legal Address",
      "mainInfoUrl": "http://r3app-server.ad-ins.com/LOS/Generic/GetDataViewBySQL",
      "mainInfo": [
        {
          "type": "text",
          "label": "ADDRESS",
          "property": "Addr"
        },
        {
          "type": "text",
          "label": "ZIP CODE",
          "property": "ZipCode"
        },
        {
          "type": "text",
          "label": "RT/RW",
          "property": "RtRw"
        },
        {
          "type": "text",
          "label": "KELURAHAN",
          "property": "Kelurahan"
        },
        {
          "type": "text",
          "label": "PHONE 1",
          "property": "Phone1"
        },
        {
          "type": "text",
          "label": "KECAMATAN",
          "property": "Kecamatan"
        },
        {
          "type": "text",
          "label": "PHONE 2",
          "property": "Phone2"
        },
        {
          "type": "text",
          "label": "CITY",
          "property": "City"
        },
        {
          "type": "text",
          "label": "FAX",
          "property": "Fax"
        }
      ],
      "querystring": {
        "select": "SELECT lca.ADDR AS Addr, lca.ZIPCODE AS ZipCode, CONCAT(lca.AREA_CODE_4, '/', lca.AREA_CODE_3) AS RtRw, lca.AREA_CODE_2 AS Kelurahan, lca.AREA_CODE_1 AS Kecamatan, lca.CITY AS City, CONCAT(lca.PHN_AREA_1,' ',lca.PHN_1, '-',lca.PHN_EXT_1) AS Phone1, CONCAT(lca.PHN_AREA_2,' ',lca.PHN_2, '-',lca.PHN_EXT_2) AS Phone2, CONCAT(lca.FAX_AREA,'-', lca.FAX) AS Fax",
        "from": "FROM dbo.LEAD_CUST_ADDR lca WITH(NOLOCK) JOIN dbo.LEAD_CUST lc WITH(NOLOCK) on lca.LEAD_CUST_ID = lc.LEAD_CUST_ID",
        "where": "WHERE lca.MR_CUST_ADDR_TYPE_CODE = 'LEGAL' AND lc.LEAD_ID={0}"
      }
    }
  ]
