          "label": "Order Information",
          "property": "MrOrderInfoCode"
        },
        {
          "type": "text",
          "label": "Asset Name",
          "property": "FullAssetName"
        },
        {
          "type": "link",
          "label": "Lead No.",
          "property": "LeadNo",
          "position": "left",
          "path": "/Lead/View",
          "target": "_blank",
          "param": [
            { 
              "type": "LeadId",
              "property": "LeadId" 
            }
          ]
        }
      ],
      "querystring": {
        "select": "SELECT a.APP_NO AS AppNo, a.APP_ID AS AppId, a.ORI_OFFICE_NAME AS OriOfficeName, b.CUST_NAME AS CustName, a.PROD_OFFERING_NAME AS ProdOfferingName, a.PROD_OFFERING_CODE AS ProdOfferingCode, a.PROD_OFFERING_VERSION AS ProdOfferingVersion, a.TENOR AS Tenor, a.CURR_CODE AS CurrCode, a.MR_ORDER_INFO_CODE AS MrOrderInfoCode, c.FULL_ASSET_NAME AS FullAssetName, d.LEAD_NO AS LeadNo, d.LEAD_ID AS LeadId",
        "from": "FROM dbo.APP a WITH(NOLOCK) LEFT JOIN dbo.APP_CUST b ON a.APP_ID = b.APP_ID LEFT JOIN dbo.APP_ASSET c ON a.APP_ID = c.APP_ID LEFT JOIN dbo.LEAD d WITH(NOLOCK) ON a.LEAD_ID = d.LEAD_ID",
        "where": "WHERE a.APP_ID = {0}"
      }
    }
  ]
