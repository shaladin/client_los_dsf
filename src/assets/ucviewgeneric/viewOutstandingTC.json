      "subSectionId": "MainInfoId",
      "subSectionTitle": "Main Information",
      "mainInfoPath": "/Generic/GetDataViewBySQL",
      "mainInfo": [
        {
          "type": "link",
          "label": "Application No",
          "property": "AppNo",
          "name": "AppNo",
          "path": "/Nap/View/AppView",
          "target" : "_blank",
          "param": [
            {
              "type": "AppId",
              "property": "AppId"
            }
          ]
        },
        {
          "type": "text",
          "label": "Customer Name",
          "property": "CustName"
        }
      ],
      "querystring": {
        "select": "SELECT A.APP_NO AS AppNo,A.APP_ID AS AppId, AG.CUST_NAME AS CustName",
        "from": "FROM dbo.APP A WITH(NOLOCK) JOIN dbo.AGRMNT AG WITH(NOLOCK) ON A.APP_ID = AG.APP_ID",
        "where": "WHERE A.APP_ID = {0}"
      }
    }
  ]
